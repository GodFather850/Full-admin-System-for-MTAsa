function SpecialAdminWarn ( warn , admin )
	outputDebugString ( ""..removeHex(warn).."" )
	local MyRank = getElementData(accSys:getPlayerAcc(admin),"pAdmin")
	for index,modira in ipairs(getElementsByType("player")) do
		if getElementData(modira,"loggedIn") == true then
			if getElementData(accSys:getPlayerAcc(modira),"pAdmin") >= MyRank then
				outputChatBox(warn, modira, 255, 255, 255, true )
			end
		end
	end
end

function SendMessageToAll ( message )
	for index,allPlayers in ipairs(getElementsByType("player")) do
		if getElementData(allPlayers,"loggedIn") and getElementData(allPlayers,"loggedIn") == true then
			outputChatBox(message, allPlayers, 255, 255, 255, true )
		end
	end
end


addCommandHandler("setnumber",function(thePlayer,cmd,target,days)
	if accSys:GetPlayerData(thePlayer,"pAdmin") >= 15 then
		if target and tonumber(days) then
			local FinalWanted = math.floor(tonumber(days))
			local taraf = miscSys:findPlayer(thePlayer,target)
			if taraf then
				if isFreePhoneNumber(FinalWanted) then
					sendPlayerMessage("#c78e8d[Phone-Number]:#ffffff Shoma Numbere #c78e8d"..getPlayerName(taraf).."#ffffff Ra Be #c78e8d"..math.floor(FinalWanted).."#ffffff Set Kardid!",thePlayer,255,255,255,true)
					accSys:SetPlayerData(taraf,"pNumber",math.floor(FinalWanted))
					sendAdminWarn("Admin #ffa700"..getPlayerName(thePlayer).."#FFFFFF Shomareye #ffa700"..getPlayerName(taraf).."#FFFFFF ra Be #ffa700"..FinalWanted.."#FFFFFF Set Kard!",accSys:GetPlayerData(thePlayer,"pAdmin"))
				else
					sendPlayerMessage("#FF0000[Error]:#FFFFFF In Number Male Kase Dige i Ast!",thePlayer,205,205,205,true)
				end
			end
		else
			sendPlayerMessage("Synatx: /setnumber <Partofname/ID> <Level>",thePlayer,205,205,205,true)
		end
	else
		sendPlayerMessage("#ff0000[Error] Shoma Dastresi Be in CMD nadarid!",thePlayer,255,255,255,true)
	end
end,false,false)



