	end
	end
	end
			else
				outputChatBox("#EE0A0AMeghdar Money Ra Vared Konid !", thePlayer, 255, 255, 255, true)
			end
			else
				outputChatBox("#EE0A0AChenin Playeri Dakhel Server Nmibashad !", thePlayer, 255, 255, 255, true)
			end
		else
			outputChatBox("#bababaBezan : /givemoney <PartOfName/ID> <Meghdar>", thePlayer, 255, 0, 0, true)
