			else
				--outputChatBox("#EE0A0ARanke Mosavi Va Bala Tar Az Khodeto Nemishe Ban Kard !", thePlayer, 255, 255, 255, true)
			end
		else
			outputChatBox("#EE0A0APlayer Peyda Nashod !", thePlayer, 255, 255, 255, true)
